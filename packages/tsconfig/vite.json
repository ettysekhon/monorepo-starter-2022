{
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ESNext",
    "allowJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "sourceMap": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "types": ["vite/client"]
  },
  "exclude": ["node_modules"]
}
